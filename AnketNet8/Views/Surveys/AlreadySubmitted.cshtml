@{
    ViewData["Title"] = "Anket Çözüldü";
}

<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>@ViewData["Title"] - Siteniz Adı</title>
</head>
<body>
    <div class="container mt-5">
        <div class="card text-center shadow-lg p-4 bg-white rounded">
            <div class="card-body">
                <h1 class="card-title text-warning">Bu Anketi Zaten Çözdünüz!</h1>
                <p class="card-text">Bu ankete daha önce katıldığınız için tekrar katılamazsınız.</p>
                <a href="/" class="btn btn-primary mt-3">Ana Sayfaya Dön</a>
            </div>
        </div>
    </div>

</body>
</html>
